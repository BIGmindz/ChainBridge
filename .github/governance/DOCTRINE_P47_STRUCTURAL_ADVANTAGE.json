  "doctrines": [
    {
      "id": "DOC-001",
      "name": "Infinite Test Suite",
      "principle": "Testing is Proof, not QA",
      "invariant": "NO TEST REGRESSION → NO MERGE → NO PAC",
      "requirements": [
        "Happy-path tests are insufficient",
        "Negative tests are mandatory",
        "Adversarial tests are mandatory",
        "Chaos tests are mandatory",
        "No PR may reduce test coverage",
        "No PR may reduce chaos coverage"
      ],
      "enforcement": {
        "trigger": "test_regression",
        "action": "BLOCK_MERGE",
        "override_requires": "LAW_TIER_PAC"
      }
    },
    {
      "id": "DOC-002",
      "name": "Chaos Coverage Index",
      "principle": "Chaos dimensions must be explicitly covered and monotonically increasing",
      "invariant": "IF CCI < THRESHOLD → EXECUTION BLOCKED; CCI MUST BE MONOTONIC",
      "dimensions": [
        {"code": "AUTH", "name": "Authentication", "description": "Identity, session, permission failures"},
        {"code": "STATE", "name": "State", "description": "Inconsistent, stale, corrupted state"},
        {"code": "CONC", "name": "Concurrency", "description": "Race conditions, deadlocks, ordering"},
        {"code": "TIME", "name": "Time", "description": "Clock skew, timeout, scheduling"},
        {"code": "DATA", "name": "Data", "description": "Malformed, missing, overflow, injection"},
        {"code": "GOV", "name": "Governance", "description": "Rule violations, policy conflicts"}
      ],
      "enforcement": {
        "trigger": "cci_decrease",
        "action": "BLOCK_MERGE",
        "surface_in": ["OCC", "CI"],
        "override_requires": "LAW_TIER_PAC"
      }
    },
    {
      "id": "DOC-003",
      "name": "100% Audit Zero Sampling",
      "principle": "All transactions audited. All decisions replayable. No sampling.",
      "invariant": "IF NOT REPLAYABLE → NOT EXECUTED",
      "requirements": [
        "Every transaction logged with cryptographic proof",
        "Every decision has complete input/output trail",
        "Every agent action has attestation",
        "Proofs are cryptographic, not textual",
        "Audit trail is tamper-evident"
      ],
      "enforcement": {
        "trigger": "audit_gap",
        "action": "BLOCK_EXECUTION",
        "override_requires": "LAW_TIER_PAC"
      }
    },
    {
      "id": "DOC-004",
      "name": "Universal Adapter Principle",
      "principle": "Any input format is acceptable. Translation is system responsibility.",
      "invariant": "UNSUPPORTED FORMAT IS SYSTEM FAILURE NOT USER FAILURE",
      "requirements": [
        "No unsupported format errors to users",
        "Format translation is automatic",
        "Integration cost trends asymptotically to zero",
        "Legacy format support is permanent"
      ],
      "enforcement": {
        "trigger": "format_rejection",
        "action": "INCIDENT_REPORT",
        "override_requires": "LAW_TIER_PAC"
      }
    },
    {
      "id": "DOC-005",
      "name": "Operator Console Supremacy",
      "principle": "Glass-box over magic. State visibility over hidden automation.",
      "invariant": "IF OPERATOR CANNOT SEE IT → IT CANNOT EXECUTE",
      "requirements": [
        "All system state visible to operators",
        "All agent decisions explainable",
        "All automation interruptible",
        "Kill-switches are first-class UX elements",
        "No hidden execution paths"
      ],
      "enforcement": {
        "trigger": "visibility_gap",
        "action": "BLOCK_EXECUTION",
        "override_requires": "LAW_TIER_PAC"
      }
    },
    {
      "id": "DOC-006",
      "name": "Fail-Closed as Revenue Strategy",
      "principle": "No silent success. No partial execution. No unverifiable outcome.",
      "invariant": "AMBIGUITY DEFAULTS TO STOP NOT GO",
      "requirements": [
        "Ambiguous states default to STOP",
        "Partial execution is failure",
        "Silent success is failure",
        "Every outcome is verifiable",
        "Every failure is explicit"
      ],
      "enforcement": {
        "trigger": "partial_execution_or_silent_success",
        "action": "ROLLBACK_AND_ALERT",
        "override_requires": "LAW_TIER_PAC"
      }
    },
    {
      "id": "DOC-007",
      "name": "AI Labor Overhead Arbitrage",
      "principle": "Explicit mandate to over-invest where humans cannot.",
      "invariant": "IF INCUMBENTS SKIPPED DUE TO COST → WE PURSUE BY DEFAULT",
      "requirements": [
        "Quality is a compounding asset",
        "Security is a compounding asset",
        "Governance is a compounding asset",
        "Documentation is a compounding asset",
        "Uneconomic rigor is compulsory"
      ],
      "enforcement": {
        "trigger": "overhead_arbitrage_opportunity",
        "action": "PRIORITIZE_IN_PAC",
        "override_requires": "LAW_TIER_PAC"
      }
    }
  ],
