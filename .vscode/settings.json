{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.venv": true,
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/*.pyc": true
  },
  "search.exclude": {
    "**/.venv": true,
    "**/node_modules": true,
    "**/__pycache__": true,
    "vscode-vfs://**": true
  },
  "markdownlint.run": "onSave",
  "markdownlint.config": {
    "default": false
  },
  "yaml.validate": true,
  "python.analysis.typeCheckingMode": "basic"
}
