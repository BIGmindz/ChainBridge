    "RULE-REG-004": {
      "name": "Format Compliance",
      "rule": "All PACs must match the canonical format pattern",
      "violation_action": "REJECT_PAC"
    },
    "RULE-REG-005": {
      "name": "Domain Code Validity",
      "rule": "Domain codes must be from approved list",
