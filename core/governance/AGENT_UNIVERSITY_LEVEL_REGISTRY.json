      "supervision_requirement": "NONE",
      "audit_frequency": "ON_ACCESS",

      "promotion_to_L1": {
        "minimum_observation_hours": 24,
        "policy_violations_allowed": 0,
        "human_approval_required": true,
        "l2_recommendation_required": false
