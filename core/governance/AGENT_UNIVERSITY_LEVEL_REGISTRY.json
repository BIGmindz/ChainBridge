      "allowed": true,
      "approvers": ["HUMAN", "L3"],
      "requirements": "promotion_to_L1"
    },
    "L1_to_L2": {
      "allowed": true,
      "approvers": ["HUMAN", "GOVERNANCE_BOARD"],
      "requirements": "promotion_to_L2"
    },
    "L2_to_L3": {
      "allowed": true,
      "approvers": ["HUMAN", "ARCHITECT"],
      "requirements": "promotion_to_L3"
    },
    "L3_to_L2": {
      "allowed": true,
      "approvers": ["HUMAN", "ARCHITECT"],
      "immediate": true
    },
    "L2_to_L1": {
      "allowed": true,
      "approvers": ["HUMAN", "L3"],
      "immediate": true
    },
    "L1_to_L0": {
      "allowed": true,
      "approvers": ["HUMAN", "L2", "L3"],
      "immediate": true
