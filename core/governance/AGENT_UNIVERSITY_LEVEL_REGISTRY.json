        },
        "GENERATE_REPORTS": {
          "description": "May generate analytical reports from observed data",
          "scope": "READ_ONLY_DATA"
        }
      },

      "restrictions": {
        "NO_EXECUTION": "Cannot execute any task or operation",
        "NO_STATE_MODIFICATION": "Cannot modify any system state",
        "NO_EXTERNAL_CALLS": "Cannot make external API or network calls",
        "NO_PDO_GENERATION": "Cannot generate Proof/Decision/Outcome records"
