    "PROBATION": {
      "description": "Elevated monitoring with restricted capabilities",
      "execution_permitted": true,
      "capabilities_reduced": true,
      "duration": "DEFINED_BY_APPROVER",
      "can_transition_to": ["FULL_LEVEL", "DOWNGRADED", "REVOKED"]
    }
  },

